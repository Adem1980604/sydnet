<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Få fat i valuta kurser</title>
</head>
<body>
  <h1>Valuta kurs søg</h1>
  <input type="text" id="valutasoeg" placeholder="Hvilken valuta vil du søge efter (f.eks., EUR)">
  <button onclick="soeg()">Søg efter valuta kurs</button>
  <ul id="resultList"></ul>

  <script>
    async function soeg() {
      const valuta = document.getElementById("valutasoeg").value;
      console.log(valuta);
      const response = await fetch(`/aktiesoeg/hentvalutakurs/${valuta}`);
      const data2 = await response.json();

      console.log("****************DEBUG 9990**************");
      console.log(data2.base_code);
      console.log(data2.conversion_rates);
      console.log("****************DEBUG 9991: EUR**************");
      console.log(data2.conversion_rates.EUR);
      console.log("****************DEBUG 9993: DKK**************");
      console.log(data2.conversion_rates.DKK);
      console.log("****************DEBUG 9994: USD**************");
      console.log(data2.conversion_rates.USD);
    }
  </script>
</body>
</html>